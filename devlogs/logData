archView = new App.Views.Search();\n\n    App.searchApp.filtersView = new App.Views.Filters({\n        parent: App.searchApp.searchView\n    });\n\n    App.searchApp.sortView = new App.Views.Sort({\n        parent: App.searchApp.searchView,\n        canonical_url: '/locuri-de-munca/it-software/it---telecom/',\n    });\n    App.searchApp.sortView.addSelectpicker();\n\n    App.searchApp.paginationView = new App.Views.Pagination({\n        parent: App.searchApp.searchView,\n        pagination_url: '/locuri-de-munca/it-software/it---telecom/'\n    });\n\n    // array care contine numele filtrelor ce pot fi populate din backend\n    \n    //initializare filtre\n                                        App.searchApp.searchView.filters['q'].push("");\n                                                                                                                    App.searchApp.searchView.filters["departament"].push({ name: "IT Software", slug: "it-software" });\n                                                                                                                                            App.searchApp.searchView.filters["industrie"].push({ name: "IT / Telecom", slug: "it---telecom" });\n                                                                        \n});\n</script>\n\n\n<script type="text/javascript">\nvar Search = Search || {};\n\nSearch.getParams = function() {\n\n    var params = [];\n    var formId = '#search-form';\n\n    if ($(formId + " input[name='q']").val() !== '') {\n        params.push('q=' + escape($(formId + " input[name='q']").val()));\n    }\n\n    var fields = [];\n            fields.push('oras');\n            fields.push('departament');\n            fields.push('nivel_cariera');\n            fields.push('industrie');\n            fields.push('tip_job');\n            fields.push('education_levels');\n    \n    for(var i in fields) {\n        $(formId + " input[name='"+ fields[i] + "[]']:checked").each(function () {\n            if ($(this).val() !== '') {\n                params.push(fields[i] + '[]=' + $(this).val());\n            }\n        });\n    }\n\n    return params;\n};\n\nvar hideSaveSearchJLComplete = function () {\n    $('.notificationV2-complete').hide();\n    $('body').removeClass('secondary-toaster-active');\n};\nloaderToaster = function () {\n    $('body').addClass('secondary-toaster-active');\n    $('.notificationV2-loading').show();\n};\nSearch.save = function() {\n\n        var params = Search.getParams();\n        if (params.length) {\n            params.push('save_search=1');\n\n            $.ajax({\n                url:        '/user/search_jobs.php',\n                type:       'post',\n                dataType:   'json',\n                data:       params.join('&'),\n                beforeSend: loaderToaster\n\n            }).done(function (responseText) {\n                $('#save-search')\n                        .addClass('saved')\n                        .blur();\n\n                                $('.notificationV2-loading').hide();\n                $('.notificationV2-complete').show();\n                setTimeout(hideSaveSearchJLComplete, 5000);\n            }).fail(function(jqXHR, textStatus, errorThrown){\n                alert("Ne pare rau, au aparut probleme la salvarea cautarii.\\n\\nVa rugam sa ne contactati in scopul raportarii acestei erori, cu urmatoarele informatii:\\nCod eroare: " + jqXHR.status + "\\nMesaj: " + errorThrown);\n            });\n        }\n        else {\n            alert('Alegeti criterii pentru cautare!');\n        }\n\n};\n\n$(document).ready(function() {\n\n    if ($.cookie().searchViewMode == 'gridview') {\n        $(".jobitem").removeClass('gridview').addClass('listview');\n        $.cookie('searchViewMode', null, { expires: -1, path: '/' });\n    }\n\n    $('#save-search').on('click', function(){\n\n        var $this = $(this);\n\n        if( !$this.hasClass('saved') ) {\n\n            var params = Search.getParams();\n\n         